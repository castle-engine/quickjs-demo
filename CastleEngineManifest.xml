<?xml version="1.0" encoding="utf-8"?>
<project name="SnakeGame"
  caption="SnakeGame"
  qualified_name="com.eugeneloza.snakegame"
  game_units="SnakeGameMain"
  >

  <icons>
    <icon path="snake_icon.png" />
  </icons>

  <ios 
    override_qualified_name="com.eugeneloza.snakegame"
    uses_non_exempt_encryption="true">
  </ios>

  <package>
    <include path="data/*" recursive="True" />
    <exclude path="*.xcf" />
    <exclude path="*.blend*" />
    <exclude path="*~" />
    <exclude path="*/.DS_Store" />
    <exclude path="*/thumbs.db" />
  </package>

  <dependencies>
    <dependency name="Png" />
    <dependency name="Sound" />
    <dependency name="OggVorbis" />
    <dependency name="FreeType" />
  </dependencies>
</project>
